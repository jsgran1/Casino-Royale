<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    android:orientation="horizontal"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/button_bar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="4dp"
        android:scaleType="fitXY"
        android:src="@drawable/button_bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/slot_title"
        android:layout_width="500dp"
        android:layout_height="75dp"
        android:src="@drawable/slot_title"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/slot_frame"
        android:layout_width="425dp"
        android:layout_height="425dp"
        android:src="@drawable/slot_frame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="55dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/lights_off"
        android:layout_width="425dp"
        android:layout_height="425dp"
        android:src="@drawable/lights_off"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="55dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/jackpot_frame_0"
        android:layout_width="239dp"
        android:layout_height="138dp"
        android:src="@drawable/jackpot_frame_0"
        app:layout_constraintBottom_toBottomOf="@id/slot_frame"
        app:layout_constraintEnd_toStartOf="@id/slot_frame"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/jackpot_title"
        android:layout_width="150dp"
        android:layout_height="50dp"
        android:src="@drawable/jackpot_title"
        app:layout_constraintBottom_toTopOf="@id/jackpot_frame_0"
        app:layout_constraintEnd_toStartOf="@id/slot_frame"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/handle_up"
        android:layout_width="206dp"
        android:layout_height="305dp"
        android:src="@drawable/handle_up"
        app:layout_constraintStart_toEndOf="@id/slot_frame"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/pay_table_up"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/pay_table_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="30dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/line_bet_up"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/line_bet_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/pay_table_up"
        android:layout_marginStart="@dimen/btn_gaps"
        android:layout_marginLeft="@dimen/btn_gaps"
        android:layout_marginBottom="10dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/line_select_up"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/line_select_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/line_bet_up"
        android:layout_marginStart="@dimen/btn_gaps"
        android:layout_marginLeft="@dimen/btn_gaps"
        android:layout_marginBottom="10dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/bet_label"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/bet_label"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/line_select_up"
        android:layout_marginStart="@dimen/btn_gaps"
        android:layout_marginLeft="@dimen/btn_gaps"
        android:layout_marginBottom="10dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/win_label"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/win_label"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/bet_label"
        android:layout_marginStart="@dimen/btn_gaps"
        android:layout_marginLeft="@dimen/btn_gaps"
        android:layout_marginBottom="10dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/cash_out_up"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:src="@drawable/cash_out_up"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/win_label"
        android:layout_marginStart="@dimen/btn_gaps"
        android:layout_marginLeft="@dimen/btn_gaps"
        android:layout_marginBottom="30dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/line_bet_txt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/credit_text"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        app:layout_constraintBottom_toTopOf="@id/line_bet_up"
        app:layout_constraintStart_toStartOf="@id/line_bet_up"
        app:layout_constraintEnd_toEndOf="@id/line_bet_up"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/times"
        android:src="@drawable/times"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@id/line_bet_txt"
        app:layout_constraintEnd_toStartOf="@id/line_select_txt"
        app:layout_constraintBottom_toTopOf="@id/line_bet_up"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/line_select_txt"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/credit_text"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        app:layout_constraintBottom_toTopOf="@id/line_select_up"
        app:layout_constraintStart_toStartOf="@id/line_select_up"
        app:layout_constraintEnd_toEndOf="@id/line_select_up"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/equals"
        android:src="@drawable/equals"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintStart_toEndOf="@id/line_select_txt"
        app:layout_constraintEnd_toStartOf="@id/bet_text"
        app:layout_constraintBottom_toTopOf="@id/line_bet_up"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/bet_text"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/bet_text"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        app:layout_constraintBottom_toTopOf="@id/bet_label"
        app:layout_constraintStart_toStartOf="@id/bet_label"
        app:layout_constraintEnd_toEndOf="@id/bet_label"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/win_txt"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/credit_text"
        app:layout_constraintTop_toBottomOf="@id/slot_frame"
        app:layout_constraintBottom_toTopOf="@id/win_label"
        app:layout_constraintStart_toStartOf="@id/win_label"
        app:layout_constraintEnd_toEndOf="@id/win_label"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/credit_label"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:scaleType="fitXY"
        android:src="@drawable/credit_label"
        app:layout_constraintBottom_toTopOf="@id/cash_out_up"
        app:layout_constraintStart_toStartOf="@id/cash_out_up"
        android:layout_marginBottom="40dp"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/credit_text"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:scaleType="fitXY"
        android:src="@drawable/credit_text"
        app:layout_constraintBottom_toTopOf="@id/credit_label"
        app:layout_constraintStart_toStartOf="@id/credit_label"
        app:layout_constraintEnd_toEndOf="@id/credit_label"
        android:layout_marginBottom="10dp"
        tools:ignore="ContentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>